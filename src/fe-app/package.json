{
  "name": "fe-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:fe-app": "node dist/fe-app/server/server.mjs",
    "start:all": "concurrently --kill-others \"npm:backend\" \"npm:frontend\"",
    "backend": "echo \"starting backend\" && cd ../be && mvn spring-boot:run",
    "frontend": "echo \"starting frontend\" && ng serve",
    "cleanup": "cd ../.."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cli": "^19.2.6",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/platform-server": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@angular/ssr": "^19.2.6",
    "@types/jasmine": "^5.1.7",
    "@types/jasminewd2": "^2.0.13",
    "express": "^4.18.2",
    "jasmine-core": "^5.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/compiler-cli": "^19.2.5",
    "@playwright/test": "^1.50.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.18.0",
    "@web/test-runner": "^0.20.0",
    "concurrently": "^9.1.0",
    "jasmine": "^5.5.0",
    "typescript": "~5.5.2"
  }
}
